public class Sid {
    public static int sumOfElements(int matrix[][]) {
        int result = 0;
      
        for (int i = 0; i < matrix.length; i++) {
          for (int j = 0; j < matrix[i].length; j++) {
            
            result += matrix[i][j];
            
          }
        }
      
        return result;
    }    
}
