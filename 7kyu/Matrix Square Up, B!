class Kata {
  static String[][] matrixSquareUp(int b) {
    String [][] matrix = new String[b][b]; 
    
    for (int i = 0; i < b; i++) {
      for (int j = b - 1; j >= 0; j--) {
        
        if (i + j + 1 < b) {
          
            matrix[i][j] = "x";
          
        }else{
          
            matrix[i][j] = b - j + "";
          
        }
        
      }
    }
    return matrix;
  }
}
